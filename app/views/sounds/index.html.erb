<div class="col s12 m12 l12">
          <h1  class= "section tooltipped" data-position="right" data-delay="50" data-tooltip="Click Play to Listen to the Hottest Tracks">TRACKS</h1>
            <div id="mu-box">
          <% @soundc.each do |x| %>
            <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/<%=x['embedhtml']%>&amp;auto_play=false&amp;hide_related=true&amp;show_comments=false&amp;buying=false&amp;sharing=false&amp;liking=false&amp;show_user=true&amp;show_reposts=false&amp;visual=false"></iframe>
            <div class="soundcloudspace"></div>
            <style>
            .cookiePolicy{
              display:none;
            }
            .sc-visuallyhidden{
                display:none;
              }
            </style>
          <% end %>
        </div>
    </div>