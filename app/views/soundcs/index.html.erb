
<div class="row" id="tracklist">
  <div class="col s12 m12 l12">
    <% @soundc.each do |x| %>
      <div>
        <h4><%= x['title']%></h4>
        <img class='materialboxed' src='<%= x['artwork_url'] %>'>
        <p class="desc"><%= x['description'].html_safe %><p>
          <div id="play-button<%= x['id'] %>" style="cursor:pointer">Start</div>
      </div>
    <% end %>
  </div>
</div>
