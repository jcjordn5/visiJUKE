<div class="row">
  <div class="col s12 m12 l12">
    <canvas id="soundhopper" style="background-color:black">
    </canvas>
    
  </div>
</div>

<div class="row">
  <div id="tracklist">
    <div class="col s12 m12 l12">
      <% @soundc[2..8].each do |x| %>
        <div id="track<%=x['id']%>" class="song-title">
          <img class='materialboxed' src='<%= x['artwork_url'] %>'>
          <p style="line-height:5; font-size:8px;"><%= x['title']%></p>
        </div>
      <% end %>
    </div>
  </div>
</div>
